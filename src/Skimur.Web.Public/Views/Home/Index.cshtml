@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_LayoutEmpty.cshtml";
}
<div id="wrapper">
    @if (Request.IsAuthenticated)
    {
        // only show sidebar of subscribed subs if authenticated
        @{Html.RenderSideBar();}
    }
    <div id="page-content-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <a href="javascript:void(0);" class="btn btn-default" id="menu-toggle">Toggle Menu</a>
                </div>
            </div>
        </div>
    </div>
</div>

<script>
    $(function () {
        $("#menu-toggle").click(function (e) {
            e.preventDefault();
            $("#wrapper").toggleClass("toggled");
        });
    });
</script>